<form method="POST" action="/poems">
  <h1>Add a Poem</h1>
  <br><br>
  <label for "title">Title:</label>
  <input type="text" name="title" id="title"> 
  <br><br><br>
  <label for "content">Content:</label>
  <br><br>
  <textarea name="content" rows="25" cols="50"></textarea>
  <br><br>
  <input type="submit">
</form>